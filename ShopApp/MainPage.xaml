<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ShopApp.MainPage">

    <VerticalStackLayout>
        <Button Text="Boton" />
        <DatePicker />
        <Editor Placeholder="Esto es un editor" HeightRequest="200" />
        <Entry Placeholder="Esto es una caja de texto o entry" />
        <Picker>
            <Picker.Items>
                <x:String>Vaxi</x:String>
                <x:String>Drez</x:String>
                <x:String>Nestor Arcila</x:String>
            </Picker.Items> 
        </Picker>
        <Stepper Minimum="0" Maximum="10" Increment="1" />
        <Switch IsToggled="False" />
        <Slider Minimum="1" Maximum="11" />
        <TimePicker />
    </VerticalStackLayout>

</ContentPage>
